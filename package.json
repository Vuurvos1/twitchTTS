{
	"name": "twitchtts",
	"version": "3.0.0",
	"description": "TwitchTTS is a tool for streamers that reads out highlighted messages form chat",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.1.1",
		"@sveltejs/kit": "^2.5.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-svelte": "^2.35.1",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^4.2.10",
		"svelte-check": "^3.6.4",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"vite": "^5.1.1"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Vuurvos1/twitchTTS.git"
	},
	"homepage": "https://github.com/Vuurvos1/twitchTTS#readme",
	"author": "Vuurvos",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/Vuurvos1/twitchTTS/issues"
	},
	"keywords": [
		"Twitch",
		"Message",
		"Chat",
		"Highlight",
		"Rewards"
	],
	"type": "module",
	"dependencies": {
		"comfy.js": "^1.1.16",
		"tmi.js": "^1.8.5"
	}
}
